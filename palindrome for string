import java.util.*;
public class Main
{
    
   
    
    
	public static void main(String[] args) {
	    
	   //palindrome for String
	   
	   String str="aabbaa";
	   str=str.toLowerCase();
	    Stack<String> st=new Stack<>();
	   boolean ans=palindrome(str,st);
	  
	   if(ans)
	   {
	       System.out.print("palindrome");
	   }else{
	       System.out.print("not palindrome");
	   }
	    
	    
	}
	
